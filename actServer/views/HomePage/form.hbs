<body>
    <header>
        <nav>
            <a href="./" class="logo">
                <img src="images/logo.jpg" />
            </a>
            <ul>
                <li><a href="./">Home</a></li>
            </ul>
        </nav>
    </header>


    <div class="main">


        <div class="container">

            <div class="header-bar"></div>

            <div class="progress-form">
                <div class="numbers">
                    <p class="setp-number" id="step_one">1</p>
                    <p class="setp-number" id="step_two">2</p>
                    <p class="setp-number" id="step_three">3</p>
                    <!-- <p class="setp-number" id="step_four">4</p> -->
                </div>
                <div class="progress-bar" id="progressBar"></div>

            </div>
            {{!-- <button id="loadData" type="button" onclick="uploading()">Upload</button> --}}
            <!-- <div class="form"> -->

            <form method="POST" action="/form" enctype="multipart/form-data">
                <div class="step-one" id="formStepOne">
                    <h3>Project details</h3>
                    <input type="text" placeholder="Project Title" id="title" name="title" required>
                    <input type="text" placeholder="Project Objective" id="obj" name="obj" minlength="80" required>
                    <div class="upload-flex">
                        <label for="myfile">Project report </label>
                        <input type="file" id="myfile" class="upload-box" name="myfile"><br>
                        <label for="myfile">Upload logo or poster of your project</label>
                        <input type="file" id="myfile1" class="upload-box" name="myfile1">
                    </div>
                    <div class="flexdropdown">
                        <div class="dropdowns">
                            <label for="dropdown1">Academic Year:</label><br>
                            <select id="dropdown1" class="select">
                                <option id="options" value="none" selected disabled hidden>Select</option>
                                <option id="options" value="2021-22">2021-22</option>
                                <option id="options" value="2022-23">2022-23</option>
                                <option id="options" value="2023-24">2023-24</option>
                            </select>
                        </div>
                        <div class="dropdowns" class="select">
                            <label for="dropdown3">Semester</label><br>
                            <select id="dropdown3">
                                <option value="none" selected disabled hidden>Select</option>
                                <option value="Semester I">Semester I</option>
                                <option value="Semester II">Semester II</option>
                            </select>
                        </div>
                        <div class="dropdowns" class="select">
                            <label for="dropdown2">Year</label><br>
                            <select id="dropdown2">
                                <option value="none" selected disabled hidden>Select</option>
                                <option value="Second Year">Second Year</option>
                                <option value="Third Year">Third Year</option>
                                <option value="B.TECH">B.TECH</option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="step-two" id="formStepTwo">
                    <div class="wrap">
                        <div class="flexwrap">
                            <h4>Team Members Information</h4>
                            <a href="#" class="add">&plus;</a>
                        </div>
                        <div class="member-info">
                            <div class="leader-info">
                                <input type="text" placeholder="Your Name" id="yname" name="yname" required>
                                <input type="text" placeholder="Your PRN" id="prn" name="prn" required>
                            </div>

                        </div>
                        <input type="text" placeholder="Your Project Guide / Mentor Name (if any)" id="gname"
                            name="gname">

                    </div>

                </div>

                <div class="step-three" id="formStepThree">
                    <!-- <input type="text" placeholder="Technology Stack Used" id="techstack"> -->
                    <div class="stackupload">
                        <h4 for="stackupload">Choose technologies you used</h4>
                        <select name="technologies" id="technologies" multiple>
                            <option value="HTML">HTML</option>
                            <option value="CSS">CSS</option>
                            <option value="Javascript">Javascript</option>
                            <option value="React">React</option>
                            <option value="Node JS">Node JS</option>
                            <option value="Flutter">Flutter</option>
                            <option value="Dart">Dart</option>
                            <option value="Deep Learning">Deep Learning</option>
                            <option value="IoT">IoT</option>
                            <option value="Python">Python</option>
                            <option value="HTML">HTML</option>
                            <option value="CSS">CSS</option>
                            <option value="Javascript">Javascript</option>
                            <option value="React">React</option>
                            <option value="Node JS">Node JS</option>
                            <option value="Flutter">Flutter</option>
                            <option value="Dart">Dart</option>
                            <option value="Deep Learning">Deep Learning</option>
                        </select>
                    </div>
                    <label for="myfile" style="margin-right: 115px">Video demo (if any)</label>
                    <input type="file" id="vid1" class="upload-box" name="vid1"><br>
                    <label for="myfile" style="margin-right: 10px">Some glimpses of your application</label>
                    <input type="file" id="apk_views" class="upload-box" name="apk_views" multiple><br>
                    <input type="text" placeholder="Project Repository link (Github)" name="repolink" id="repolink">
                    <div class="flex-input">
                        <input type="text" placeholder="Website link" name="weblink" id="weblink">
                        {{!-- <label for="myfile" style="margin-left: 50px">App apk</label> --}}
                        {{!-- <input type="file" id="myfile" class="upload-box" name="apkf"><br> --}}
                        {{!-- <button id="done" type="submit" onclick="postData()">Done</button> --}}
                    </div>

                    <!-- <div class="buttons" id="buttonsCTN">
                        <button id="prev" type="submit">Previous</button>
                        <button id="next" type="submit" onclick="myfunction()">Next</button>
                    </div> -->
                </div>

                <div class="buttons" id="buttonsCTN">
                    <button id="prev" type="button">Previous</button>
                    <button id="next" type="button" onclick="myfunction()">Next</button>
                    <button id="done" type="submit" onclick="postData()">Done</button>
                </div>
            </form>

            {{!-- <div class="buttons" id="buttonsCTN">
                <button id="prev" type="button">Previous</button>
                <button id="next" type="button" onclick="myfunction()">Next</button>
                <button id="done" type="submit" onclick="postData()">Done</button>

            </div> --}}

        </div>



    </div>


    <script src="javascripts/form.js"></script>
    <script src="javascripts/multi-select-tag.js"></script>
    <script>
        new MultiSelectTag('technologies')  // id
    </script>




</body>

</html>


























































































